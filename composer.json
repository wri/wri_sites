{
    "name": "wri/wri_sites",
    "description": "WRI Drupal 8 profile",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/views_arg_order_sort",
                "type": "drupal-module",
                "version": "dev-8.x-1.x",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/views_arg_order_sort.git",
                    "reference": "5d13cfca5749294d127ebc06348827f775342b37"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "patches": {
            "drupal/core": {
                "Dynamic Views Arguments via Tokens https://www.drupal.org/project/drupal/issues/1699378": "https://www.drupal.org/files/issues/2024-07-07/1699378-drupal-10.3.x.patch",
                "Add reusable option to inline block creation https://www.drupal.org/project/drupal/issues/2999491": "https://www.drupal.org/files/issues/2024-01-23/add_reusable_option_to_inline_block_creation-2999491-141.patch",
                "Hyphens and forward slashes (-/) break Views contextual filters https://www.drupal.org/node/672606": "https://www.drupal.org/files/issues/2022-09-27/672606-76.patch",
                "Views RSS feeds have validation issues https://www.drupal.org/project/drupal/issues/1337894": "https://git.drupalcode.org/project/drupal/-/merge_requests/10628.diff",
                "Add missing enclosure to Views RSS https://www.drupal.org/node/2511878": "https://www.drupal.org/files/issues/2024-06-23/2511878-add-support-for-enclosure-field-10.3.x.patch",
                "Views Term ID has broken filters (\"All of\", \"Is none of\") and contextual filters \"allow multiple\" https://www.drupal.org/project/drupal/issues/2829178": "https://git.drupalcode.org/project/drupal/-/merge_requests/5321.patch",
                "More link is missing in pager when using the \"Some\" pager and there are more records than shown https://www.drupal.org/project/drupal/issues/3381979": "https://www.drupal.org/files/issues/2023-10-09/3381979-13.patch"
            },
            "drupal/config_partial_export": {
                "Support config download based on collection https://www.drupal.org/project/config_partial_export/issues/3439357": "https://git.drupalcode.org/project/config_partial_export/-/merge_requests/6.diff"
            },
            "drupal/content_snippets": {
                "Error: Cannot unset string offsets... https://www.drupal.org/project/content_snippets/issues/3268757": "https://git.drupalcode.org/issue/content_snippets-3268757/-/merge_requests/1.diff"
            },
            "drupal/diff": {
                "No local tasks on compare revisions page https://www.drupal.org/project/diff/issues/3169729": "https://www.drupal.org/files/issues/2020-09-08/diff_add-local-tasks-3169729-2.patch"
            },
            "drupal/entity_embed": {
                "Logs flooded with warning messages 'Invalid display settings encountered' https://www.drupal.org/project/entity_embed/issues/3077225": "https://www.drupal.org/files/issues/2019-12-11/3077225-10.reduce-invalid-config-logs.patch"
            },
            "drupal/layout_builder_st": {
                "Error: Call to a member function getConfig() OverridesSectionStorage.php https://www.drupal.org/project/layout_builder_st/issues/3420063": "https://www.drupal.org/files/issues/2024-03-29/layout_builder_st-call_to_a_member_function_getconfig-3420063-26.patch",
                "Contextual links for translation are removed by core https://www.drupal.org/project/layout_builder_st/issues/3411037": "https://www.drupal.org/files/issues/2024-05-23/3411037-contextual-links-removed-from-core.patch",
                "Drupal\\Component\\Plugin\\Exception\\PluginNotFoundException: The \"block.settings.<block_name>\" plugin does not exist https://www.drupal.org/project/layout_builder_st/issues/3463435": "https://www.drupal.org/files/issues/2024-07-23/layout_builder_st-plugin-not-found-3463435-3.patch"
            },
            "drupal/metatag": {
                "Provide field formatter to output meta tags on-page, e.g. for use with Views (D9), https://www.drupal.org/project/metatag/issues/3027873": "https://www.drupal.org/files/issues/2023-07-23/metatag-field_formatter_for_views-3027873-27.patch"
            },
            "drupal/schema_metatag": {
                "Description sub_property for Organization / Person: https://www.drupal.org/project/schema_metatag/issues/3187882": "https://www.drupal.org/files/issues/2021-01-04/description-sub-property-for-organization-or-person-3187882-4.patch",
                "allow more characters in \"sameAs\" field? or convert to multivalue single fields? https://www.drupal.org/project/schema_metatag/issues/3498969": "https://git.drupalcode.org/project/schema_metatag/-/merge_requests/90.diff"
            },
            "drupal/oembed_lazyload": {
                "Use strict should be inside function https://www.drupal.org/project/oembed_lazyload/issues/3475956#comment-16067120": "https://www.drupal.org/files/issues/2024-09-21/oembed_lazyload_use_strict-3475956.patch"
            },
            "drupal/paragraphs": {
                "Paragraph access check using incorrect revision of its parent https://www.drupal.org/project/paragraphs/issues/3090200": "https://www.drupal.org/files/issues/2022-10-26/3084934-13_combine_3090200-22.patch",
                "Add a parent entity token for paragraphs https://www.drupal.org/project/paragraphs/issues/3067265": "https://git.drupalcode.org/project/paragraphs/-/merge_requests/92.diff"
            },
            "drupal/search_api": {
                "View recalculated with wrong data from cache https://www.drupal.org/project/search_api/issues/3327645": "https://git.drupalcode.org/project/search_api/-/merge_requests/63.diff"
            },
            "drupal/token": {
                "Add a [current-page:object:?] dynamic token https://www.drupal.org/project/token/issues/919760": "https://www.drupal.org/files/issues/2020-09-08/token--current_page_object_token--919760--83.patch",
                "Provide array of primitive property values on multivalue field https://www.drupal.org/project/token/issues/3115486": "https://www.drupal.org/files/issues/2020-12-11/token-provide_primitive_type_arrays_for_properties_of_multivalue_fields-3115486-12.patch",
                "Updating to Token 1.13 breaks smart_date range tokens https://www.drupal.org/project/token/issues/3396209": "https://www.drupal.org/files/issues/2023-11-27/token-3396209-bypassing-null-insertion.patch"
            },
            "drupal/views_arg_order_sort": {
                "Set as compatible with Drupal 10 but lock down version https://www.drupal.org/project/views_arg_order_sort/issues/3212018" : "https://raw.githubusercontent.com/wri/wri_sites/refs/heads/main/patches/d10-upgrade-circumvent-3212018.diff"
            },
            "drupal/views_bulk_edit": {
                "Fix Required fields to really not be required https://www.drupal.org/node/3024419": "https://www.drupal.org/files/issues/2024-01-10/views_bulk_edit-3024419-fix_required_fields-57.patch"
            },
            "drupal/views_extras": {
                "Why does Views Extras require Drupal_Visitors to be part of the cookie name? https://www.drupal.org/project/views_extras/issues/3054202" : "https://www.drupal.org/files/issues/2024-01-03/3054202-11.patch"
            },
            "drupal/layout_builder_modal": {
                "incompatibility with layout_builder_st, contextual links broken https://www.drupal.org/project/layout_builder_modal/issues/3133695": "https://www.drupal.org/files/issues/2022-12-15/3133695-13.patch"
            },
            "drupal/webform": {
                "[Webform 6.2.3] Element autocomplete does not validate against list of options https://www.drupal.org/project/webform/issues/3463864": "https://git.drupalcode.org/project/webform/-/merge_requests/493.diff?date=2024-07-16"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "project-root": "./web",
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
